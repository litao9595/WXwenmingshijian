                <view class="right">
                    <input type="text" placeholder="请输入负责人身份证"/>
                </view>
            </view>
            <view class="textarea-list">
                <view class="top">团队简介</view>
                <view class="bottom">
                    <textarea placeholder="请输入团队简介"></textarea>
                </view>
            </view>
            <view class="submit-btn">提交</view>
        </view>
    </view>
</view>

